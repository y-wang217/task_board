doctype html
head
  meta(charset='utf-8')
  meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
  meta(name='description' content='')
  meta(name='author' content='')
  title Deloitte Task Board
  link(rel='icon' href='https://media-exp1.licdn.com/dms/image/C560BAQGJHYXy9e1rEg/company-logo_200_200/0?e=1590019200&v=beta&t=37LxLqSVBwOt6F3DMfw4v6cRWU84ydgbbbrePFutb8k' type='image/x-icon')
  // Bootstrap core CSS
  link(rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css' integrity='sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh' crossorigin='anonymous')
