// Navigation
nav.navbar.navbar-expand-lg.navbar-dark.bg-dark.static-top
  .container
    a.navbar-brand(href='#') Deloitte
    button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarResponsive' aria-controls='navbarResponsive' aria-expanded='false' aria-label='Toggle navigation')
      span.navbar-toggler-icon
    #navbarResponsive.collapse.navbar-collapse
      ul.navbar-nav.ml-auto
        li.nav-item.active
          a.nav-link(href='#')
            | Home
            span.sr-only (current)
        li.nav-item
          a.nav-link(href='#') Your Tasks
        li.nav-item
          a.nav-link(href='#') Completed Tasks
        li.nav-item
          button.btn.btn-outline-success#create_task(type='button') Create Task
#myModal.modal
  // Modal content
  .modal-content
    button#close(style='float: right;') X
    form(action='#')
      | Hours:
      br
      input(type='text' name='estimate_time_cost' value='')
      br
      | Due Date:
      br
      input(type='date' name='due_date' value='')
      br
      | Description:
      br
      input(type='text' name='task_description' value='')
      br
      br
      input#submit_task(type='submit' value='Submit')

